gem_hostname_windows=mars

if [[ ${gem_os} == "Linux" ]]; then

    sudo umount ${HOME}/gem/home/${USER}/mnt/${gem_hostname_windows}_msys64_${USER}
    sudo umount ${HOME}/gem/home/${USER}/mnt/${gem_hostname_windows}_root
fi
